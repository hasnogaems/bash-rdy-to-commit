{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-11 сборка активного файла",
            "command": "/usr/local/bin/gcc-11",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "cat.c",
                "parse_flags.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Задача создана отладчиком."
        }
    ],
    "version": "2.0.0"
}